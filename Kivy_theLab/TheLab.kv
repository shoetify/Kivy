#:import layout_examples layout_examples
#:import widget_examples widget_examples
#:import navigation_screen_manager navigation_screen_manager

MyScreenManager:

<MyScreenManager@NavigationScreenManager>:
    Screen:
        name: "screen1"
        BoxLayout:
            Button:
                text: "go to ImagesExample"
                on_press: app.manager.push("ImagesExample")
            Button:
                text: "go to screen 3"
                on_press: app.manager.push("screen3")
    Screen:
        name: "ImagesExample"
        BoxLayout:
            orientation: "vertical"
            ActionBar:
                ActionView:
                    ActionPrevious:
                        title: "images"
                        on_release: app.manager.pop()
            ImagesExample:
    Screen:
        name: "screen2"
        Button:
            text: "Button2"
            on_press: app.manager.pop()
    Screen:
        name: "screen3"
        Button:
            text: "Button3"
            on_press: app.manager.pop()
